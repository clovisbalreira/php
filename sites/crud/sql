CREATE TABLE users('id' INTEGER PRIMARY KEY,'name' VARCHAR(100) NOT NULL,'email' varchar(100) NOT NULL);
CREATE TABLE colors(id INTEGER PRIMARY KEY,name VARCHAR(50) NOT NULL);
CREATE TABLE user_colors(user_id INTEGER NOT NULL,color_id INTEGER NOT NULL);
